<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
			
		-->
		
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.CommentToStringConverter" />
		<bean class="converters.ConfigurationToStringConverter" />
		<bean class="converters.EventToStringConverter" />
		<bean class="converters.FertilizerToStringConverter" />
		<bean class="converters.GardenerToStringConverter" />
		<bean class="converters.MeasurementToStringConverter" />
		<bean class="converters.MessageEmailToStringConverter" />
		<bean class="converters.PlantToStringConverter" />
		<bean class="converters.PredictionToStringConverter" />
		<bean class="converters.TasteToStringConverter" />
		<bean class="converters.TimeTableToStringConverter" />
		<bean class="converters.WateringAreaToStringConverter" />
		
		<bean class="converters.StringToActorConverter" /> 
		<bean class="converters.StringToAdministratorConverter" /> 
		<bean class="converters.StringToCommentConverter" /> 
		<bean class="converters.StringToConfigurationConverter" /> 
		<bean class="converters.StringToEventConverter" /> 
		<bean class="converters.StringToFertilizerConverter" /> 
		<bean class="converters.StringToGardenerConverter" /> 
		<bean class="converters.StringToMeasurementConverter" /> 
		<bean class="converters.StringToMessageEmailConverter" /> 
		<bean class="converters.StringToPlantConverter" /> 
		<bean class="converters.StringToPredictionConverter" /> 
		<bean class="converters.StringToTasteConverter" /> 
		<bean class="converters.StringToTimeTableConverter" /> 
		<bean class="converters.StringToWateringAreaConverter" /> 

	</util:list>
	
</beans>